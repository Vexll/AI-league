        "        \"preferences\": {\n",
        "            \"food\": food_interest,\n",
        "            \"sightseeing\": sightseeing_interest,\n",
        "            \"fanzones\": fanzone_interest,\n",
        "            \"shopping\": shopping_interest,\n",
