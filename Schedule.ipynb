        "        \"special_requests\": special_requests\n",
        "    }\n",
        "\n",
        "def generate_itinerary(inputs):\n",
        "    \"\"\"Generate detailed itinerary using GPT-4-turbo with comprehensive inputs\"\"\"\n",
        "    # Format ticket details for the prompt\n",
        "    ticket_info = \"N/A\"\n",
        "    if inputs[\"has_ticket\"] and inputs[\"ticket_details\"]:\n",
        "        ticket_info = \"\\n\".join([\n",
        "            f\"- Event: {t['event']}, Date: {t['date']}, Time: {t['time']}, Venue: {t['venue']}, Seat: {t['seat']}\"\n",
        "            for t in inputs[\"ticket_details\"]\n",
        "        ])\n",
        "\n",
        "    # Format preferences for the prompt\n",
        "    preference_ratings = []\n",
        "    for pref, rating in inputs[\"preferences\"].items():\n",
        "        stars = \"★\" * rating + \"☆\" * (5 - rating)\n",
        "        preference_ratings.append(f\"{pref.capitalize()}: {stars} ({rating}/5)\")\n",
        "\n",
        "    preference_info = \"\\n\".join(preference_ratings)\n",
        "\n",
        "    prompt = f\"\"\"\n",
        "Create a detailed sports event roadmap for a fan with the following details:\n",
        "\n",
        "BASIC INFORMATION:\n",
        "- Trip start date: {inputs['start_date']}\n",
        "- Trip end date: {inputs['end_date']}\n",
        "- Duration: {inputs['duration']} days\n",
        "- Location: {inputs['location']}\n",
        "- Event type: {inputs['event_type']}\n",
        "- Event name: {inputs['event_name']}\n",
        "\n",
        "LOGISTICS:\n",
        "- Transport mode: {inputs['transport_mode']}\n",
        "- Accommodation: {inputs['accommodation']}\n",
        "- Accommodation location: {inputs['accommodation_location']}\n",
        "\n",
        "TICKETS:\n",
        "- Has tickets: {'Yes' if inputs['has_ticket'] else 'No'}\n",
        "- Ticket details: {ticket_info}\n",
        "{f\"- Ticket interests: {inputs['ticket_interest']}\" if not inputs['has_ticket'] else \"\"}\n",
        "\n",
        "PREFERENCES:\n",
        "{preference_info}\n",
        "- Dietary restrictions: {inputs['dietary_restrictions']}\n",
        "- Mobility limitations: {inputs['mobility_limitations']}\n",
        "- Budget level: {inputs['budget_level']}\n",
        "- Special requests: {inputs['special_requests']}\n",
        "\n",
        "INSTRUCTIONS:\n",
        "1. Create a simplified roadmap of 5 main activities during the trip\n",
        "2. Focus on a mix of sports events and other important activities\n",
        "3. Keep descriptions concise but meaningful\n",
        "4. Ensure each entry has a clear category that describes the type of activity\n",
        "\n",
        "Format each entry with:\n",
        "NUMBER||EVENT_NAME||EVENT_DESCRIPTION||CATEGORY||DATE\n",
        "\n",
        "Categories should be one of: SPORT, SHOPPING, FOOD, SIGHTSEEING, EVENT, TRANSPORT, ENTERTAINMENT, FAN_EXPERIENCE, ACCOMMODATION, OTHER\n",
        "\n",
        "Example format:\n",
        "1||World Cup Opening Match||Brazil vs Germany at Lusail Stadium||SPORT||2025-03-31\n",
        "2||Downtown Shopping Tour||Explore the central market for souvenirs||SHOPPING||2025-04-01\n",
        "\"\"\"\n",
        "\n",
        "    response = openai.ChatCompletion.create(\n",
        "        model=\"gpt-4-turbo\",\n",
        "        messages=[{\"role\": \"user\", \"content\": prompt}],\n",
        "        max_tokens=1000\n",
        "    )\n",
        "\n",
        "    return response.choices[0].message.content\n",
        "\n",
        "def create_roadmap_image(itinerary_text, user_inputs, output_filename=\"sports_roadmap.png\"):\n",
        "    \"\"\"\n",
        "    Create a roadmap image where events are grouped by date into a double-line row:\n",
        "      - The top line shows a horizontal timeline with event times.\n",
        "      - The bottom line displays event detail boxes containing the event name (activity) and location.\n",
        "    The design uses rounded rectangles for detail boxes and refined spacing.\n",
        "    \"\"\"\n",
        "    import datetime, textwrap\n",
        "    from PIL import Image, ImageDraw, ImageFont\n",
        "\n",
        "    # Parse itinerary text into a list of event dictionaries.\n",
        "    lines = [line.strip() for line in itinerary_text.split('\\n') if line.strip()]\n",
        "    events = []\n",
        "    for line in lines:\n",
        "        parts = line.split('||')\n",
        "        if len(parts) >= 7:\n",
        "            events.append({\n",
        "                \"day\": parts[0],\n",
        "                \"date\": parts[1],\n",
        "                \"time\": parts[2],\n",
        "                \"activity\": parts[3],\n",
        "                \"duration\": parts[4],\n",
        "                \"category\": parts[5],\n",
        "                \"location\": parts[6]\n",
        "            })\n",
        "    if not events:\n",
        "        print(\"No itinerary data found.\")\n",
        "        return\n",
        "\n",
        "    # Group events by date.\n",
        "    grouped = {}\n",
        "    for ev in events:\n",
        "        key = ev[\"date\"]\n",
        "        grouped.setdefault(key, []).append(ev)\n",
        "\n",
        "    # Sort dates chronologically.\n",
        "    sorted_dates = sorted(grouped.keys(), key=lambda d: datetime.datetime.strptime(d, '%Y-%m-%d'))\n",
        "\n",
        "    # IMAGE CONFIGURATION\n",
        "    image_width = 2000\n",
        "    header_height = 150\n",
        "    day_block_height = 220  # Height for each day row (double-line)\n",
        "    vertical_spacing = 40\n",
        "    total_days = len(sorted_dates)\n",
        "    image_height = header_height + total_days * (day_block_height + vertical_spacing) + 100\n",
        "\n",
        "    background_color = (245, 245, 245)\n",
        "    timeline_color = (150, 150, 150)\n",
        "\n",
        "    # Load fonts.\n",
        "    try:\n",
        "        title_font = ImageFont.truetype(\"arial.ttf\", 48)\n",
        "        day_font = ImageFont.truetype(\"arial.ttf\", 32)\n",
        "        time_font = ImageFont.truetype(\"arial.ttf\", 24)\n",
        "        detail_font = ImageFont.truetype(\"arial.ttf\", 22)\n",
        "    except OSError:\n",
        "        title_font = ImageFont.load_default()\n",
        "        day_font = ImageFont.load_default()\n",
        "        time_font = ImageFont.load_default()\n",
        "        detail_font = ImageFont.load_default()\n",
        "\n",
        "    image = Image.new(\"RGB\", (image_width, image_height), background_color)\n",
        "    draw = ImageDraw.Draw(image)\n",
        "\n",
        "    # HEADER: Title, Subtitle, and Date Range.\n",
        "    title = \"Sports Event Roadmap\"\n",
        "    subtitle = f\"{user_inputs['event_name']} in {user_inputs['location']}\"\n",
        "    date_range = f\"{user_inputs['start_date']} to {user_inputs['end_date']}  •  {user_inputs['duration']} days\"\n",
        "\n",
        "    bbox = draw.textbbox((0, 0), title, font=title_font)\n",
        "    title_w = bbox[2] - bbox[0]\n",
        "    title_h = bbox[3] - bbox[1]\n",
        "    draw.text(((image_width - title_w) / 2, 20), title, font=title_font, fill=(239, 71, 58))\n",
        "\n",
        "    bbox = draw.textbbox((0, 0), subtitle, font=day_font)\n",
        "    subtitle_w = bbox[2] - bbox[0]\n",
        "    subtitle_h = bbox[3] - bbox[1]\n",
        "    draw.text(((image_width - subtitle_w) / 2, 20 + title_h + 10), subtitle, font=day_font, fill=(30,30,30))\n",
        "\n",
        "    bbox = draw.textbbox((0, 0), date_range, font=detail_font)\n",
        "    date_w = bbox[2] - bbox[0]\n",
        "    date_h = bbox[3] - bbox[1]\n",
        "    draw.text(((image_width - date_w) / 2, 20 + title_h + subtitle_h + 20),\n",
        "              date_range, font=detail_font, fill=(80,80,80))\n",
        "\n",
        "    # Starting y position for day blocks.\n",
        "    current_y = header_height\n",
        "\n",
        "    day_count = 1\n",
        "    for date_key in sorted_dates:\n",
        "        day_events = grouped[date_key]\n",
        "        day_events.sort(key=lambda ev: ev[\"time\"])\n",
        "\n",
        "        # Format the date nicely.\n",
        "        try:\n",
        "            date_obj = datetime.datetime.strptime(date_key, '%Y-%m-%d')\n",
        "            formatted_date = date_obj.strftime('%A, %B %d, %Y')\n",
        "        except ValueError:\n",
        "            formatted_date = date_key\n",
        "        day_header = f\"Day {day_count} – {formatted_date}\"\n",
        "        draw.text((20, current_y + 20), day_header, font=day_font, fill=(239, 71, 58))\n",
        "\n",
        "        # Define timeline area for this day.\n",
        "        timeline_start_x = 350\n",
        "        timeline_end_x = image_width - 50\n",
        "        timeline_y = current_y + 60  # Top row: timeline for event times.\n",
        "        draw.line([(timeline_start_x, timeline_y), (timeline_end_x, timeline_y)], fill=timeline_color, width=3)\n",
        "\n",
        "        num_events = len(day_events)\n",
        "        spacing = (timeline_end_x - timeline_start_x) / (num_events) if num_events > 0 else 0\n",
        "\n",
        "        # Draw event markers and times above the timeline.\n",
        "        for idx, ev in enumerate(day_events):\n",
        "            center_x = timeline_start_x + spacing * (idx + 0.5)\n",
        "            node_radius = 10\n",
        "            draw.ellipse((center_x - node_radius, timeline_y - node_radius,\n",
        "                          center_x + node_radius, timeline_y + node_radius), fill=(62, 163, 69))\n",
        "            time_str = ev[\"time\"]\n",
        "            bbox = draw.textbbox((0, 0), time_str, font=time_font)\n",
        "            time_w = bbox[2] - bbox[0]\n",
        "            time_h = bbox[3] - bbox[1]\n",
        "            draw.text((center_x - time_w/2, timeline_y - node_radius - time_h - 5),\n",
        "                      time_str, font=time_font, fill=(30,30,30))\n",
        "\n",
        "        # Bottom row: Draw event detail boxes.\n",
        "        detail_y = current_y + 130\n",
        "        for idx, ev in enumerate(day_events):\n",
        "            center_x = timeline_start_x + spacing * (idx + 0.5)\n",
        "            # Build detail text: use only the event name and location.\n",
        "            detail_lines = [f\"{ev['activity']}\"]\n",
        "            if ev[\"location\"].strip():\n",
        "                detail_lines.append(f\"{ev['location']}\")\n",
        "            wrapped_text = \"\\n\".join([textwrap.fill(line, width=30) for line in detail_lines])\n",
        "\n",
        "            bbox = draw.multiline_textbbox((0,0), wrapped_text, font=detail_font, spacing=4)\n",
        "            detail_w = bbox[2] - bbox[0]\n",
        "            detail_h = bbox[3] - bbox[1]\n",
        "            padding = 8\n",
        "            # Draw a rounded rectangle for the detail box.\n",
        "            rect_coords = [center_x - detail_w/2 - padding, detail_y - padding,\n",
        "                           center_x + detail_w/2 + padding, detail_y + detail_h + padding]\n",
        "            # Using rounded_rectangle if available (Pillow 8.2.0+)\n",
        "            try:\n",
        "                draw.rounded_rectangle(rect_coords, radius=10, fill=(255,255,255), outline=(200,200,200))\n",
        "            except AttributeError:\n",
        "                draw.rectangle(rect_coords, fill=(255,255,255), outline=(200,200,200))\n",
        "            draw.multiline_text((center_x - detail_w/2, detail_y),\n",
        "                                wrapped_text, font=detail_font, fill=(30,30,30), spacing=4)\n",
        "\n",
        "        current_y += day_block_height + vertical_spacing\n",
        "        day_count += 1\n",
        "\n",
        "    image.save(output_filename)\n",
        "    print(f\"Roadmap saved as {output_filename}\")\n",
        "    return image\n",
        "# For running interactively:\n",
        "def main():\n",
        "    try:\n",
        "        openai.api_key = input(\"Enter your OpenAI API key: \")\n",
        "        user_inputs = collect_user_inputs()\n",
        "        print(\"\\nGenerating your sports event roadmap...\")\n",
        "        itinerary = generate_itinerary(user_inputs)\n",
        "        print(\"\\nGenerated Itinerary Data:\")\n",
        "        print(itinerary)\n",
        "        print(\"\\nCreating your high-quality visual roadmap...\")\n",
        "        create_roadmap_image(itinerary, user_inputs)\n",
        "        print(\"\\nYour sports event roadmap has been created successfully!\")\n",
        "        show_image = input(\"\\nWould you like to view the roadmap now? (yes/no): \").lower() == 'yes'\n",
        "        if show_image:\n",
        "            try:\n",
        "                Image.open(\"sports_roadmap.png\").show()\n",
        "            except Exception as e:\n",
        "                print(f\"Couldn't display the image: {e}\")\n",
        "                print(\"Please open the sports_roadmap.png file manually.\")\n",
        "    except Exception as e:\n",
        "        print(f\"An error occurred: {e}\")\n"
