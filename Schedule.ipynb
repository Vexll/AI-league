        "\n",
        "def generate_itinerary(inputs):\n",
        "    \"\"\"Generate detailed itinerary using GPT-4-turbo with comprehensive inputs\"\"\"\n",
        "    # Format ticket details for the prompt\n",
        "    ticket_info = \"N/A\"\n",
        "    if inputs[\"has_ticket\"] and inputs[\"ticket_details\"]:\n",
        "        ticket_info = \"\\n\".join([\n",
        "            f\"- Event: {t['event']}, Date: {t['date']}, Time: {t['time']}, Venue: {t['venue']}, Seat: {t['seat']}\"\n",
        "            for t in inputs[\"ticket_details\"]\n",
        "        ])\n",
        "\n",
        "    # Format preferences for the prompt\n",
        "    preference_ratings = []\n",
        "    for pref, rating in inputs[\"preferences\"].items():\n",
        "        stars = \"★\" * rating + \"☆\" * (5 - rating)\n",
        "        preference_ratings.append(f\"{pref.capitalize()}: {stars} ({rating}/5)\")\n",
        "\n",
        "    preference_info = \"\\n\".join(preference_ratings)\n",
        "\n",
        "    prompt = f\"\"\"\n",
        "    Create a detailed sports event roadmap for a fan with the following details:\n",
        "\n",
        "    BASIC INFORMATION:\n",
        "    - Trip start date: {inputs['start_date']}\n",
        "    - Trip end date: {inputs['end_date']}\n",
        "    - Duration: {inputs['duration']} days\n",
        "    - Location: {inputs['location']}\n",
        "    - Event type: {inputs['event_type']}\n",
        "    - Event name: {inputs['event_name']}\n",
        "\n",
        "    LOGISTICS:\n",
        "    - Transport mode: {inputs['transport_mode']}\n",
        "    - Accommodation: {inputs['accommodation']}\n",
        "    - Accommodation location: {inputs['accommodation_location']}\n",
        "\n",
        "    TICKETS:\n",
        "    - Has tickets: {'Yes' if inputs['has_ticket'] else 'No'}\n",
        "    - Ticket details: {ticket_info}\n",
        "    {f\"- Ticket interests: {inputs['ticket_interest']}\" if not inputs['has_ticket'] else \"\"}\n",
        "\n",
        "    PREFERENCES:\n",
        "    {preference_info}\n",
        "    - Dietary restrictions: {inputs['dietary_restrictions']}\n",
        "    - Mobility limitations: {inputs['mobility_limitations']}\n",
        "    - Budget level: {inputs['budget_level']}\n",
        "    - Special requests: {inputs['special_requests']}\n",
        "\n",
        "    INSTRUCTIONS:\n",
        "    1. Create a chronological roadmap of activities from arrival to departure\n",
        "    2. Include both sports events and complementary activities based on preferences\n",
        "    3. Provide specific timings for each activity with reasonable travel times\n",
        "    4. For food, suggest actual restaurant names appropriate to the preferences and budget\n",
        "    5. If no tickets, suggest ticket sources and viewing alternatives (sports bars, fan zones)\n",
        "    6. Incorporate any special requests where feasible\n",
        "\n",
        "    Format each entry with:\n",
        "    DAY||DATE||TIME||ACTIVITY||DURATION||CATEGORY||LOCATION\n",
        "\n",
        "    Categories should be one of: ARRIVAL, DEPARTURE, EVENT, FOOD, SIGHTSEEING, FAN_EXPERIENCE, TRANSPORT, REST, SHOPPING, ENTERTAINMENT\n",
        "\n",
        "    Example format:\n",
        "    1||2025-03-31||12:00 PM||Arrive at Riyadh International Airport||1h||ARRIVAL||Riyadh International Airport\n",
        "    1||2025-03-31||2:00 PM||Check into Hotel (includes 30 min travel from airport)||1h||TRANSPORT||Hyatt Regency Riyadh\n",
        "    \"\"\"\n",
        "\n",
        "    response = openai.ChatCompletion.create(\n",
        "        model=\"gpt-4-turbo\",\n",
        "        messages=[{\"role\": \"user\", \"content\": prompt}],\n",
        "        max_tokens=2000\n",
        "    )\n",
        "\n",
        "    return response.choices[0].message.content\n",
        "\n",
        "def create_roadmap_image(itinerary_text, user_inputs, output_filename=\"sports_roadmap.png\"):\n",
        "    \"\"\"Create a visual roadmap style image using PIL\"\"\"\n",
        "    # Parse the itinerary text\n",
        "    lines = [line.strip() for line in itinerary_text.split('\\n') if line.strip()]\n",
        "    data = []\n",
        "    for line in lines:\n",
        "        parts = line.split('||')\n",
        "        if len(parts) >= 7:  # Ensure we have all the necessary parts\n",
        "            data.append({\n",
        "                \"day\": parts[0],\n",
        "                \"date\": parts[1],\n",
        "                \"time\": parts[2],\n",
        "                \"activity\": parts[3],\n",
        "                \"duration\": parts[4],\n",
        "                \"category\": parts[5],\n",
        "                \"location\": parts[6]\n",
        "            })\n",
        "\n",
        "    if not data:\n",
        "        print(\"No itinerary data found.\")\n",
        "        return\n",
        "\n",
        "    # Create groups by day\n",
        "    days = {}\n",
        "    for item in data:\n",
        "        day = item[\"day\"]\n",
        "        if day not in days:\n",
        "            days[day] = []\n",
        "        days[day].append(item)\n",
        "\n",
        "    # Image settings\n",
        "    width = 1200\n",
        "    day_height = 320  # Height allocated per day\n",
        "    vertical_spacing = 80  # Space between days\n",
        "    total_days = len(days)\n",
        "\n",
        "    # Calculate total height based on number of days and spacing\n",
        "    height = (day_height * total_days) + (vertical_spacing * (total_days - 1)) + 200  # Extra space for header and footer\n",
        "\n",
        "    # Colors\n",
        "    background_color = (245, 245, 250)  # Light blue-gray\n",
        "    title_color = (30, 50, 100)  # Dark blue\n",
        "    day_title_color = (50, 80, 140)  # Medium blue\n",
        "    timeline_color = (100, 150, 200)  # Light blue\n",
        "\n",
        "    # Category colors\n",
        "    category_colors = {\n",
        "        \"ARRIVAL\": (75, 181, 67),       # Green\n",
        "        \"DEPARTURE\": (75, 181, 67),     # Green\n",
        "        \"EVENT\": (214, 40, 40),         # Red\n",
        "        \"FOOD\": (245, 160, 25),         # Orange\n",
        "        \"SIGHTSEEING\": (180, 120, 200), # Purple\n",
        "        \"FAN_EXPERIENCE\": (214, 40, 40),# Red\n",
        "        \"TRANSPORT\": (75, 181, 67),     # Green\n",
        "        \"REST\": (100, 180, 240),        # Sky blue\n",
        "        \"SHOPPING\": (245, 160, 25),     # Orange\n",
        "        \"ENTERTAINMENT\": (180, 120, 200)# Purple\n",
        "    }\n",
        "\n",
        "    # Create image and drawing context\n",
        "    image = Image.new('RGB', (width, height), background_color)\n",
        "    draw = ImageDraw.Draw(image)\n",
        "\n",
        "    # Load fonts\n",
        "    try:\n",
        "        title_font = ImageFont.truetype(\"arial.ttf\", 48)\n",
        "        day_font = ImageFont.truetype(\"arial.ttf\", 36)\n",
        "        time_font = ImageFont.truetype(\"arial.ttf\", 28)\n",
        "        text_font = ImageFont.truetype(\"arial.ttf\", 24)\n",
        "        small_font = ImageFont.truetype(\"arial.ttf\", 20)\n",
        "    except:\n",
        "        # Fallback to default font\n",
        "        title_font = ImageFont.load_default()\n",
        "        day_font = ImageFont.load_default()\n",
        "        time_font = ImageFont.load_default()\n",
        "        text_font = ImageFont.load_default()\n",
        "        small_font = ImageFont.load_default()\n",
        "\n",
        "    # Draw title\n",
        "    title = f\"Sports Event Roadmap: {user_inputs['event_name']} in {user_inputs['location']}\"\n",
        "    title_width = draw.textlength(title, font=title_font)\n",
        "    draw.text(((width - title_width) / 2, 40), title, font=title_font, fill=title_color)\n",
        "\n",
        "    # Draw subtitle with dates\n",
        "    subtitle = f\"{user_inputs['start_date']} to {user_inputs['end_date']} ({user_inputs['duration']} days)\"\n",
        "    subtitle_width = draw.textlength(subtitle, font=day_font)\n",
        "    draw.text(((width - subtitle_width) / 2, 100), subtitle, font=day_font, fill=day_title_color)\n",
        "\n",
        "    # Draw days\n",
        "    y_offset = 180  # Starting y position after title and subtitle\n",
        "\n",
        "    # Sort days numerically\n",
        "    sorted_days = sorted(days.keys(), key=lambda x: int(x) if x.isdigit() else float('inf'))\n",
        "\n",
        "    for day in sorted_days:\n",
        "        day_items = days[day]\n",
        "\n",
        "        # Parse the date from the first item\n",
        "        date_text = day_items[0][\"date\"]\n",
        "        try:\n",
        "            date_obj = datetime.datetime.strptime(date_text, '%Y-%m-%d')\n",
        "            formatted_date = date_obj.strftime('%A, %B %d, %Y')  # e.g., \"Monday, March 31, 2025\"\n",
        "        except:\n",
        "            formatted_date = date_text\n",
        "\n",
        "        # Draw day header\n",
        "        day_title = f\"Day {day}: {formatted_date}\"\n",
        "        draw.text((50, y_offset), day_title, font=day_font, fill=day_title_color)\n",
        "\n",
        "        # Draw timeline\n",
        "        timeline_y = y_offset + 60\n",
        "        timeline_x_start = 100\n",
        "        timeline_x_end = width - 100\n",
        "        timeline_length = timeline_x_end - timeline_x_start\n",
        "\n",
        "        # Draw the main timeline line\n",
        "        draw.line([(timeline_x_start, timeline_y), (timeline_x_end, timeline_y)],\n",
        "                 fill=timeline_color, width=5)\n",
        "\n",
        "        # Sort items by time\n",
        "        day_items.sort(key=lambda x: x[\"time\"])\n",
        "\n",
        "        # Calculate positions on timeline\n",
        "        num_items = len(day_items)\n",
        "        spacing = timeline_length / (num_items + 1)\n",
        "\n",
        "        # Draw items on timeline\n",
        "        for i, item in enumerate(day_items):\n",
        "            # Calculate position\n",
        "            x_pos = timeline_x_start + spacing * (i + 1)\n",
        "\n",
        "            # Draw node on timeline\n",
        "            node_radius = 15\n",
        "            category = item[\"category\"]\n",
        "            node_color = category_colors.get(category, (100, 100, 100))\n",
        "            draw.ellipse((x_pos - node_radius, timeline_y - node_radius,\n",
        "                         x_pos + node_radius, timeline_y + node_radius),\n",
        "                         fill=node_color)\n",
        "\n",
        "            # Alternate between above and below the timeline for text\n",
        "            if i % 2 == 0:\n",
        "                # Draw above the timeline\n",
        "                text_y = timeline_y - 100\n",
        "                # Draw time\n",
        "                time_text = item[\"time\"]\n",
        "                time_width = draw.textlength(time_text, font=time_font)\n",
        "                draw.text((x_pos - time_width/2, text_y), time_text, font=time_font, fill=node_color)\n",
        "\n",
        "                # Draw activity (wrapped text)\n",
        "                activity_text = item[\"activity\"]\n",
        "                wrapped_text = textwrap.fill(activity_text, width=20)\n",
        "                draw.text((x_pos - 100, text_y + 35), wrapped_text, font=text_font, fill=(0, 0, 0))\n",
        "\n",
        "                # Draw connector line\n",
        "                draw.line([(x_pos, text_y + 30), (x_pos, timeline_y - node_radius)],\n",
        "                         fill=node_color, width=2)\n",
        "            else:\n",
        "                # Draw below the timeline\n",
        "                text_y = timeline_y + 30\n",
        "                # Draw time\n",
        "                time_text = item[\"time\"]\n",
        "                time_width = draw.textlength(time_text, font=time_font)\n",
        "                draw.text((x_pos - time_width/2, text_y), time_text, font=time_font, fill=node_color)\n",
        "\n",
        "                # Draw activity (wrapped text)\n",
        "                activity_text = item[\"activity\"]\n",
        "                wrapped_text = textwrap.fill(activity_text, width=20)\n",
        "                draw.text((x_pos - 100, text_y + 35), wrapped_text, font=text_font, fill=(0, 0, 0))\n",
        "\n",
        "                # Draw location in smaller font\n",
        "                location_text = item[\"location\"]\n",
        "                wrapped_location = textwrap.fill(location_text, width=25)\n",
        "                draw.text((x_pos - 100, text_y + 90), wrapped_location, font=small_font, fill=(80, 80, 80))\n",
        "\n",
        "                # Draw connector line\n",
        "                draw.line([(x_pos, timeline_y + node_radius), (x_pos, text_y)],\n",
        "                         fill=node_color, width=2)\n",
        "\n",
        "        # Update y_offset for next day\n",
        "        y_offset += day_height + vertical_spacing\n",
        "\n",
        "    # Draw legend\n",
        "    legend_y = height - 100\n",
        "    legend_x = 100\n",
        "    legend_spacing = 200\n",
        "\n",
        "    draw.text((50, legend_y - 40), \"Legend:\", font=day_font, fill=day_title_color)\n",
        "\n",
        "    for i, (category, color) in enumerate(category_colors.items()):\n",
        "        x = legend_x + (i % 3) * legend_spacing\n",
        "        y = legend_y + (i // 3) * 30\n",
        "\n",
        "        # Draw color circle\n",
        "        draw.ellipse((x - 10, y - 10, x + 10, y + 10), fill=color)\n",
        "\n",
        "        # Draw category name\n",
        "        draw.text((x + 20, y - 10), category, font=small_font, fill=(0, 0, 0))\n",
        "\n",
        "    # Save the image\n",
        "    image.save(output_filename)\n",
        "    print(f\"Roadmap saved as {output_filename}\")\n",
        "    return image\n",
        "\n",
